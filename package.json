{
  "name": "@retort-js/playground",
  "version": "0.0.1-dev.1",
  "description": "A place to play with Prompt Chains in retort-js",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npx retort-dev-server\" \"npm run browser-sync\"",
    "browser-sync": "browser-sync start --proxy 'localhost:3333' --files './retort/*' --no-open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retort-js/playground.git"
  },
  "author": "Daniel Huggins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/retort-js/playground/issues"
  },
  "homepage": "https://github.com/retort-js/playground#readme",
  "dependencies": {
    "@retort-js/dev-server": "latest",
    "browser-sync": "^3.0.2",
    "concurrently": "^8.2.2",
    "retort-js": "latest"
  }
}
